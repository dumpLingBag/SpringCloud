<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rngay.service_user.dao.UserDao">
    <select id="pageList" resultType="UAUserDTO">
        select * from ua_user
        <where>
            <if test="pageList.enable != null">
                enable = #{pageList.enable}
            </if>
            <if test="pageList.nickName != null and pageList.nickName != ''">
                nick_name = #{pageList.nickName}
            </if>
            <if test="pageList.username != null and pageList.username != ''">
                username = #{pageList.username}
            </if>
        </where>
    </select>
</mapper>